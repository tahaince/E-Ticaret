<body>


    <div class="container-login100" style="background-image: url('images/bg-01.jpg');">
        <div class="wrap-login100 p-l-55 p-r-55 p-t-80 p-b-30">
            <img src="https://logos.textgiraffe.com/logos/logo-name/Taha-designstyle-summer-m.png"/>
            @using (Html.BeginForm("PersonelLogin", "Personel", FormMethod.Post))
            {
        <form class="login100-form validate-form">
            <span class="login100-form-title p-b-37">
                
            </span>

            <div class="wrap-input100 validate-input m-b-20" data-validate="Enter username or email">
                <input class="input100" type="text" name="MAIL" placeholder="username or email">
                <span class="focus-input100"></span>
            </div>

            <div class="wrap-input100 validate-input m-b-25" data-validate="Enter password">
                <input class="input100" type="password" name="SIFRE" placeholder="password">
                <span class="focus-input100"></span>
            </div>

            <div class="container-login100-form-btn">
                <button class="btn btn-info">
                    GİRİŞ YAP
                </button>
            </div>
            
            <div class="text-center p-t-57 p-b-20">
                <span class="txt1">
                    Or login with
                </span>
            </div>

            <div class="flex-c p-b-112">
                <a href="https://github.com/tahaince" class="login100-social-item">
                    <i class="fa fa-github"></i>
                </a>

                <a href="https://www.linkedin.com/in/taha-ince-315411217/" class="login100-social-item">
                    <i class="fa fa-linkedin-square"></i>
                </a>
            </div>

            <div class="text-center">
                <a href="#" class="txt2 hov1">
                    Sign Up
                </a>
            </div>

        </form>

            }
        </div>
    </div>



    <div id="dropDownSelect1"></div>

